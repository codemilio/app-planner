generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Trip {
  id           String   @id @default(uuid())
  is_confirmed Boolean  @default(false)
  created_at   DateTime @default(now())
  starts_at    DateTime
  ends_at      DateTime
  destination  String

  @@map("trips")
}
